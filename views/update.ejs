<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <title>Document</title>
</head>
<body>
  <% all.forEach(function(v){ %>
    <div class="container">
      <form method="post" action="/update/:_id">
        <div class="mb-3">
          <label for="exampleInputEmail1" class="form-label">  <b>name</b> </label>
          <input value=" <%= v.name %>"  name="name" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
          <div id="emailHelp" class="form-text">We'll share your details with anyone else.</div>
        </div>
        <div class="mb-3">
          <label   for="exampleInputPassword1" class="form-label"><b>desc</b> </label>
          <input value="<%= v.desc %>" name="desc" type="text" class="form-control" id="exampleInputPassword1">
        </div>
        <div class="mb-3">
          <label for="exampleInputPassword1" class="form-label"> <b>image url</b></label>
          <input value="<%= v.imgurl %>" name="imgurl" type="text" class="form-control" id="exampleInputPassword1">
        </div>
          <div class="mb-3">
          <label for="exampleInputPassword1" class="form-label"> <b>price</b> </label>
          <input value="<%= v.price %>" name="price" type="number" class="form-control" id="exampleInputPassword1">
        </div>
        
       
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
    <% }) %>

  <hr>
  


         
       </div>
     </div>
   </div>

</body>
</html>